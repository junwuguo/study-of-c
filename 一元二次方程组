#include<stdio.h>
#include<math.h>
int main()
{
	int a, b, c, d,e;
	scanf_s("%d %d %d", &a, &b, &c);
	d = b * b - 4 * a * c;
	if (d >= 0) {
		e = sqrt(d);
		int g = (b + d) / 2*a;
		int h = (b - d) / 2*a;
		printf("resolution1=%d\nresolution2=%d", g, h);
	}

	else {
		printf("no resolution");
	}
	return 0;
}
